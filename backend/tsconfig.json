{
  "compilerOptions": {
    "target": "ES5",
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "baseUrl": ".",
    "outDir": "dist",
    "resolveJsonModule": true,
    "jsx": "preserve",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "typeRoots": ["./node_modules/@types"],
  },
  "include": [
    "index.ts", "routes/geojson.ts", "routes/status.ts"
  ]
}